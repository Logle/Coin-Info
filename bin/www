#!/usr/bin/env node

var debug = require('debug')('CoinInfo');
var app = require('../app');
var data_service = require('../data_service/start');

app.set('port', process.env.PORT || 3000);

var server = app.listen(app.get('port'), function() {
  debug('Express server listening on port ' + server.address().port);
});

 // Scraping data - comment out this function if data mining is not needed.

var adayinmilisecond = 1000*60*60*18;
setInterval(data_service.init(1,2), adayinmilisecond) ;





